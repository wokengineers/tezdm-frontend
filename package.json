{
  "name": "tezdm-frontend",
  "version": "1.0.0",
  "description": "Instagram Automation Platform Frontend",
  "private": true,
  "dependencies": {
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.0",
    "postcss": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.0",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:http": "react-scripts start",
    "start:https": "HTTPS=true SSL_CRT_FILE=localhost.pem SSL_KEY_FILE=localhost-key.pem react-scripts start",
    "setup:https": "chmod +x scripts/setup-https.sh && ./scripts/setup-https.sh",
    "build": "GENERATE_SOURCEMAP=false CI=false react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "npx eslint src --ext .ts,.tsx --fix",
    "type-check": "npx tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0"
  }
}
